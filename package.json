{
  "name": "vue-boilerplate",
  "version": "1.2.0",
  "description": "This is a boilerplate for vue project. You could use it as a base to build your own web app.",
  "author": "William Chan <root@williamchan.me>, Emil Zhai <root@derzh.com>",
  "private": true,
  "scripts": {
    "start": "npm run setup; npm run dev",
    "setup": "if [ ! -f node_modules/package.gitlab-ci.json ] || ! cmp -s package.json node_modules/package.gitlab-ci.json\\\n  || [ ! -f node_modules/package-lock.gitlab-ci.json ] || ! cmp -s package-lock.json node_modules/package-lock.gitlab-ci.json\nthen\n  cp -rf package.json node_modules/package.gitlab-ci.json.tmp\n  cp -rf package-lock.json node_modules/package-lock.gitlab-ci.json.tmp\n  npm config set registry https://registry.npm.taobao.org\n  npm install\n  mv -f node_modules/package.gitlab-ci.json.tmp node_modules/package.gitlab-ci.json\n  mv -f node_modules/package-lock.gitlab-ci.json.tmp node_modules/package-lock.gitlab-ci.json\nfi",
    "dev": "cross-env NODE_ENV=development node build/setup-dev-server.js",
    "build": "cross-env NODE_ENV=production node build/setup-build.js",
    "build:development": "cross-env NODE_ENV=development node build/setup-build.js",
    "build:clear": "rm -rf dist",
    "lint": "eslint --ext .js,.vue src"
  },
  "engines": {
    "node": ">=8.0",
    "npm": ">=5.0"
  },
  "dependencies": {
    "axios": "^0.16.2",
    "cookie": "^0.3.1",
    "cube-ui": "^1.9.3",
    "element-ui": "^1.4.2",
    "fastclick": "^1.0.6",
    "mand-mobile": "^1.2.3",
    "moment": "^2.18.1",
    "normalize.css": "^7.0.0",
    "qs": "^6.5.0",
    "vue": "^2.5.16",
    "vue-router": "^3.0.1",
    "vuex": "^3.0.1",
    "vuex-router-sync": "^5.0.0"
  },
  "devDependencies": {
    "autoprefixer": "^7.1.3",
    "babel-core": "^6.26.0",
    "babel-eslint": "^7.2.3",
    "babel-loader": "^7.1.2",
    "babel-plugin-component": "^1.1.1",
    "babel-plugin-import": "^1.7.0",
    "babel-plugin-transform-modules": "^0.1.0",
    "babel-plugin-transform-runtime": "^6.23.0",
    "babel-preset-env": "^1.4.0",
    "babel-preset-stage-2": "^6.22.0",
    "babel-register": "^6.22.0",
    "chalk": "^1.1.3",
    "connect-history-api-fallback": "^1.3.0",
    "copy-webpack-plugin": "^4.0.1",
    "cross-env": "^5.0.1",
    "css-loader": "^0.28.0",
    "eruda": "^1.3.0",
    "eslint": "^3.19.0",
    "eslint-config-airbnb-base": "^11.1.3",
    "eslint-friendly-formatter": "^2.0.7",
    "eslint-import-resolver-webpack": "^0.8.1",
    "eslint-loader": "^1.7.1",
    "eslint-plugin-html": "^2.0.0",
    "eslint-plugin-import": "^2.2.0",
    "eventsource-polyfill": "^0.9.6",
    "express": "^4.15.4",
    "extract-text-webpack-plugin": "^3.0.0",
    "file-loader": "^0.11.2",
    "friendly-errors-webpack-plugin": "^1.6.1",
    "html-webpack-plugin": "^2.28.0",
    "http-proxy-middleware": "^0.17.3",
    "node-sass": "^4.5.2",
    "opn": "^5.1.0",
    "optimize-css-assets-webpack-plugin": "^1.3.0",
    "ora": "^1.2.0",
    "postcss-loader": "^2.0.5",
    "postcss-px2rem": "^0.3.0",
    "px2rem-loader": "^0.1.7",
    "rimraf": "^2.6.1",
    "sass-loader": "^6.0.6",
    "semver": "^5.3.0",
    "shelljs": "^0.7.6",
    "stylus": "^0.54.5",
    "stylus-loader": "^2.1.1",
    "url-loader": "^0.5.9",
    "vue-loader": "^14.2.2",
    "vue-style-loader": "^4.1.0",
    "vue-template-compiler": "^2.5.16",
    "webpack": "^3.5.5",
    "webpack-bundle-analyzer": "^2.9.0",
    "webpack-dev-middleware": "^1.12.0",
    "webpack-hot-middleware": "^2.18.2",
    "webpack-merge": "^4.0.0",
    "webpack-post-compile-plugin": "^0.3.1",
    "webpack-transform-modules-plugin": "^0.3.2"
  },
  "compileDependencies": [
    "cube-ui"
  ],
  "transformModules": {
    "cube-ui": {
      "transform": "cube-ui/src/modules/${member}",
      "kebabCase": true
    }
  },
  "browserslist": [
    "> 1%",
    "last 2 versions",
    "not ie <= 8"
  ]
}
