    "webpack-filter-warnings-plugin": "^1.2.1",
    "webpack-hot-middleware": "^2.25.0",
    "webpack-merge": "^4.2.2",
    "webpackbar": "^4.0.0",
    "workbox-webpack-plugin": "^6.5.4",
    "yargs": "^16.2.0"
  }
}
