{
  "name": "vue-boilerplate",
  "title": "Vue Boilerplate",
  "version": "1.2.0",
  "description": "This is a boilerplate for vue project. You could use it as a base to build your own web app.",
  "author": "William Chan <root@williamchan.me>, Emil Zhai <root@derzh.com>",
  "private": true,
  "scripts": {
    "prepare": "husky install",
    "setup": "sh ./bin/setup.sh",
    "start": "npm run setup && npm run watch --",
    "watch": "npm run watch:development --",
    "watch:development": "node webpack/runner.js run development",
    "watch:production": "node webpack/runner.js run production",
    "build": "npm run build:production --",
    "build:development": "node webpack/runner.js build development",
    "build:production": "node webpack/runner.js build production",
    "lint": "npm run eslint && npm run tslint && npm run stylelint",
    "lint:fix": "npm run eslint:fix ; npm run stylelint:fix",
    "gitlint": "sh ./bin/gitlint.sh || exit 1",
    "tslint": "sh ./bin/tslint.sh **/*.ts",
    "tslint:commit": "sh ./bin/tslint.sh",
    "tslint:exec": "tsc --project .tsconfig-lint.json --skipLibCheck --noEmit",
    "eslint": "eslint \"**/*.{js,jsx,ts,tsx,json,.vue}\" --format pretty",
    "eslint:fix": "eslint \"**/*.{js,jsx,ts,tsx,json,.vue}\" --format pretty --fix",
    "eslint:commit": "eslint --format pretty",
    "stylelint": "stylelint \"src/**/*.{css,less,sass,scss,vue}\" --custom-formatter=node_modules/stylelint-formatter-pretty",
    "stylelint:fix": "stylelint \"src/**/*.{css,less,sass,scss,vue}\" --custom-formatter=node_modules/stylelint-formatter-pretty --fix",
    "stylelint:commit": "stylelint --custom-formatter=node_modules/stylelint-formatter-pretty",
    "commit": "cz"
  },
