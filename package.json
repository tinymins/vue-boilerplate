{
  "name": "vue-boilerplate",
  "version": "1.2.0",
  "description": "This is a boilerplate for vue project. You could use it as a base to build your own web app.",
  "author": "William Chan <root@williamchan.me>, Emil Zhai <root@derzh.com>",
  "private": true,
  "scripts": {
    "start": "npm run setup && npm run dev",
    "setup": "bash setup.sh",
    "dev": "cross-env NODE_ENV=development node build/setup-dev-server.js",
    "build": "cross-env NODE_ENV=production node build/setup-build.js",
    "build:development": "cross-env NODE_ENV=development node build/setup-build.js",
    "build:clear": "rm -rf dist",
    "lint": "eslint --ext .js,.vue src"
  },
  "engines": {
    "node": ">=8.0",
    "npm": ">=5.0"
  },
  "dependencies": {
    "axios": "^0.18.0",
    "cookie": "^0.3.1",
    "cube-ui": "^1.10.10",
    "element-ui": "^1.4.2",
    "fastclick": "^1.0.6",
    "lodash": "^4.17.10",
    "mand-mobile": "^1.2.3",
    "moment": "^2.22.2",
    "normalize.css": "^8.0.0",
    "qs": "^6.5.2",
    "vue": "^2.5.17",
    "vue-router": "^3.0.1",
    "vuex": "^3.0.1",
    "vuex-router-sync": "^5.0.0"
  },
  "devDependencies": {
    "babel-core": "^6.26.3",
    "babel-eslint": "^7.2.3",
    "babel-loader": "^7.1.5",
    "babel-plugin-component": "^1.1.1",
    "babel-plugin-import": "^1.8.0",
    "babel-plugin-transform-modules": "^0.1.1",
    "babel-plugin-transform-runtime": "^6.23.0",
    "babel-preset-env": "^1.7.0",
    "babel-preset-stage-2": "^6.24.1",
    "babel-register": "^6.26.0",
    "chalk": "^2.4.1",
    "compression-webpack-plugin": "^1.1.11",
    "connect-history-api-fallback": "^1.5.0",
    "copy-webpack-plugin": "^4.5.2",
    "cross-env": "^5.2.0",
    "css-loader": "^0.28.11",
    "eruda": "^1.5.0",
    "eslint": "^4.19.1",
    "eslint-config-airbnb-base": "^13.0.0",
    "eslint-friendly-formatter": "^4.0.1",
    "eslint-import-resolver-webpack": "^0.10.1",
    "eslint-loader": "^1.9.0",
    "eslint-plugin-import": "^2.13.0",
    "eslint-plugin-vue": "^4.7.1",
    "eventsource-polyfill": "^0.9.6",
    "express": "^4.16.3",
    "extract-text-webpack-plugin": "^3.0.2",
    "file-loader": "^1.1.11",
    "friendly-errors-webpack-plugin": "^1.7.0",
    "html-webpack-plugin": "^3.2.0",
    "http-proxy-middleware": "^0.18.0",
    "less": "^3.8.1",
    "less-loader": "^4.1.0",
    "node-sass": "^4.9.2",
    "opn": "^5.3.0",
    "optimize-css-assets-webpack-plugin": "^3.2.0",
    "ora": "^3.0.0",
    "postcss-html": "^0.32.0",
    "postcss-import": "^12.0.0",
    "postcss-less": "^2.0.0",
    "postcss-loader": "^2.1.6",
    "postcss-preset-env": "^5.3.0",
    "postcss-px-to-viewport": "0.0.3",
    "postcss-px2rem": "^0.3.0",
    "postcss-sass": "^0.3.2",
    "postcss-scss": "^2.0.0",
    "postcss-syntax": "^0.32.0",
    "rimraf": "^2.6.2",
    "sass-loader": "^7.1.0",
    "semver": "^5.5.0",
    "shelljs": "^0.8.2",
    "stylelint": "^9.4.0",
    "stylelint-bare-webpack-plugin": "^0.1.3",
    "stylelint-config-standard": "^18.2.0",
    "stylus": "^0.54.5",
    "stylus-loader": "^3.0.2",
    "url-loader": "^1.0.1",
    "vue-loader": "^15.3.0",
    "vue-style-loader": "^4.1.1",
    "vue-template-compiler": "^2.5.17",
    "webpack": "^3.12.0",
    "webpack-bundle-analyzer": "^2.13.1",
    "webpack-dev-middleware": "^1.12.0",
    "webpack-hot-middleware": "^2.18.2",
    "webpack-merge": "^4.0.0",
    "webpack-post-compile-plugin": "^0.3.1",
    "webpack-transform-modules-plugin": "^0.3.2"
  },
  "compileDependencies": [
    "cube-ui"
  ],
  "transformModules": {
    "cube-ui": {
      "transform": "cube-ui/src/modules/${member}",
      "kebabCase": true
    }
  },
  "browserslist": [
    "> 1%",
    "last 2 versions",
    "not ie <= 8"
  ]
}
