{
  "name": "vue-boilerplate",
  "version": "1.2.0",
  "description": "This is a boilerplate for vue project. You could use it as a base to build your own web app.",
  "author": "William Chan <root@williamchan.me>, Emil Zhai <root@derzh.com>",
  "private": true,
  "scripts": {
    "setup": "sh ./cmd/setup.sh",
    "start": "npm run setup && npm run watch",
    "watch": "npm run watch:development --",
    "watch:development": "node webpack/runner.js --action=run --env=development --",
    "watch:production": "node webpack/runner.js --action=run --env=production --",
    "build": "npm run build:production --",
    "build:development": "node webpack/runner.js --action=build --env=development --public-path=/m/ --",
    "build:production": "node webpack/runner.js --action=build --env=production --public-path=/m/ --",
    "build:report": "node webpack/runner.js --action=build --env=production --public-path=/m/ --report --",
    "build:chrome-ext": "node webpack/runner.js --action=build --env=production --chrome-ext --",
    "lint": "eslint --ext .js,.json,.jsx,.ts,.tsx,.tx,.vue --cache --cache-location node_modules/.cache/.eslintcache src",
    "lint:style": "stylelint src/**/*.{less,vue} --fix"
  },
  "engines": {
    "node": ">=8.0",
    "npm": ">=5.0"
  },
  "dependencies": {
