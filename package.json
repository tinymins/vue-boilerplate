{
  "name": "vue-boilerplate",
  "version": "1.2.0",
  "description": "This is a boilerplate for vue project. You could use it as a base to build your own web app.",
  "author": "William Chan <root@williamchan.me>, Emil Zhai <root@derzh.com>",
  "private": true,
  "scripts": {
    "start": "yarn run setup && yarn run hot",
    "setup": "yarn",
    "hot": "cross-env NODE_ENV=development NODE_ACTION=run PUBLIC_PATH=/ node build/setup-dev-server.js",
    "hot:production": "cross-env NODE_ENV=production NODE_ACTION=run PUBLIC_PATH=/ node build/setup-dev-server.js",
    "build": "rimraf dist && cross-env NODE_ENV=production NODE_ACTION=build PUBLIC_PATH=/m/ webpack --config build/webpack.prod.build.conf.js --hide-modules",
    "build:rc": "rimraf dist && cross-env NODE_ENV=production NODE_ACTION=build PUBLIC_PATH=/om/ webpack --config build/webpack.prod.build.conf.js --hide-modules",
    "build:development": "rimraf dist && cross-env NODE_ENV=development NODE_ACTION=build PUBLIC_PATH=/m/ webpack --config build/webpack.dev.build.conf.js --hide-modules",
    "build:chrome-ext": "rimraf dist && cross-env NODE_ENV=production NODE_ACTION=build PUBLIC_PATH=/ ROUTER_MODE=hash webpack --config build/webpack.chrome-ext.build.conf.js --hide-modules",
    "lint": "eslint --ext .js,.vue --cache --cache-location node_modules/.cache/.eslintcache src",
    "lint:style": "stylelint src/**/*.{less,vue} --fix"
  },
  "engines": {
    "node": ">=8.0",
    "npm": ">=5.0"
  },
  "dependencies": {
    "axios": "^0.18.0",
    "fastclick": "^1.0.6",
    "js-cookie": "^2.2.0",
    "lodash": "^4.17.10",
    "moment": "^2.23.0",
    "normalize.css": "^8.0.0",
    "querystringify": "^2.1.0",
    "safe-area-insets": "^1.3.1",
    "ua-parser-js": "^0.7.19",
    "url-parse": "^1.4.4",
    "uuid": "^3.3.2",
    "vue": "^2.6.6",
    "vue-class-component": "^7.0.1",
    "vue-lazyload": "^1.2.6",
    "vue-photoswipe.js": "^2.0.15",
    "vue-prevent-overscroll.js": "^2.0.1",
    "vue-property-decorator": "^8.0.0",
    "vue-router": "^3.0.1",
    "vue-transfer-dom.js": "^2.0.4",
    "vue-wechat": "^1.4.4",
    "vuex": "^3.0.1",
    "vuex-class": "^0.3.2",
    "vuex-router-sync": "^5.0.0"
  },
  "devDependencies": {
    "@babel/core": "^7.2.2",
    "@babel/plugin-proposal-decorators": "^7.3.0",
    "@babel/plugin-syntax-dynamic-import": "^7.2.0",
    "@babel/plugin-syntax-jsx": "^7.2.0",
    "@babel/polyfill": "^7.2.5",
    "@babel/preset-env": "^7.3.4",
    "@babel/preset-typescript": "^7.1.0",
    "@babel/register": "^7.0.0",
    "@typescript-eslint/eslint-plugin": "^1.4.2",
    "@typescript-eslint/parser": "^1.4.2",
    "autoprefixer": "^9.1.0",
    "babel-helper-vue-jsx-merge-props": "^2.0.3",
    "babel-loader": "^8.0.5",
    "babel-plugin-syntax-jsx": "^6.18.0",
    "babel-plugin-transform-modules": "^0.1.1",
    "babel-plugin-transform-vue-jsx": "^3.7.0",
    "cache-loader": "^2.0.1",
    "chalk": "^2.4.1",
    "compression-webpack-plugin": "^1.1.11",
    "connect-history-api-fallback": "^1.5.0",
    "copy-webpack-plugin": "^4.5.2",
    "cross-env": "^5.2.0",
    "css-loader": "^2.1.1",
    "eruda": "^1.5.0",
    "eslint": "^5.14.1",
    "eslint-config-airbnb-base": "^13.1.0",
    "eslint-friendly-formatter": "^4.0.1",
    "eslint-import-resolver-webpack": "^0.10.1",
    "eslint-loader": "^2.1.2",
    "eslint-plugin-import": "^2.16.0",
    "eslint-plugin-vue": "^5.2.2",
    "eventsource-polyfill": "^0.9.6",
    "express": "^4.16.3",
    "file-loader": "^2.0.0",
    "friendly-errors-webpack-plugin": "^1.7.0",
    "html-webpack-plugin": "^3.2.0",
    "http-proxy-middleware": "^0.18.0",
    "image-webpack-loader": "^4.6.0",
    "mini-css-extract-plugin": "^0.5.0",
    "node-sass": "^4.11.0",
    "opn": "^5.3.0",
    "optimize-css-assets-webpack-plugin": "^5.0.1",
