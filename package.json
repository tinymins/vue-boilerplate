{
  "name": "vue-boilerplate",
  "version": "1.2.0",
  "description": "This is a boilerplate for vue project. You could use it as a base to build your own web app.",
  "author": "William Chan <root@williamchan.me>, Emil Zhai <root@derzh.com>",
  "private": true,
  "scripts": {
    "start": "yarn run setup && yarn run watch",
    "setup": "yarn",
    "watch": "cross-env NODE_ENV=development NODE_ACTION=run PUBLIC_PATH=/ ROUTER_MODE=auto webpack-dev-server --inline --config build/client/webpack.dev.run.conf.js",
    "watch:production": "cross-env NODE_ENV=production NODE_ACTION=run PUBLIC_PATH=/ ROUTER_MODE=auto node build/setup-dev-server.js",
    "build": "rimraf dist && cross-env NODE_ENV=production NODE_ACTION=build PUBLIC_PATH=/m/ ROUTER_MODE=auto webpack --config build/client/webpack.prod.build.conf.js --hide-modules",
    "build:rc": "rimraf dist && cross-env NODE_ENV=production NODE_ACTION=build PUBLIC_PATH=/om/ ROUTER_MODE=auto webpack --config build/client/webpack.prod.build.conf.js --hide-modules",
    "build:development": "rimraf dist && cross-env NODE_ENV=development NODE_ACTION=build PUBLIC_PATH=/m/ ROUTER_MODE=auto webpack --config build/client/webpack.dev.build.conf.js --hide-modules",
    "build:chrome-ext": "rimraf dist && cross-env NODE_ENV=production NODE_ACTION=build PUBLIC_PATH=/ ROUTER_MODE=hash webpack --config build/client/webpack.chrome-ext.build.conf.js --hide-modules",
    "lint": "eslint --ext .js,.vue --cache --cache-location node_modules/.cache/.eslintcache src",
    "lint:style": "stylelint src/**/*.{less,vue} --fix"
  },
  "engines": {
    "node": ">=8.0",
    "npm": ">=5.0"
  },
  "dependencies": {},
  "devDependencies": {
    "@babel/core": "^7.8.3",
    "@babel/plugin-syntax-dynamic-import": "^7.8.3",
    "@babel/preset-env": "^7.8.3",
    "@babel/preset-typescript": "^7.8.3",
    "@babel/register": "^7.8.3",
    "@types/cheerio": "^0.22.15",
    "@typescript-eslint/eslint-plugin": "^2.9.0",
    "@typescript-eslint/parser": "^2.9.0",
    "@vue/babel-preset-jsx": "^1.1.2",
    "autoprefixer": "^9.7.3",
    "axios": "^0.19.1",
    "babel-eslint": "^10.0.3",
    "babel-loader": "^8.0.6",
    "cache-loader": "^4.1.0",
    "chalk": "^3.0.0",
    "cheerio": "^1.0.0-rc.3",
    "compression-webpack-plugin": "^3.0.0",
    "connect-history-api-fallback": "^1.6.0",
    "copy-webpack-plugin": "^5.0.5",
