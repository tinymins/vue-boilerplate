{
  "name": "vue-boilerplate",
  "title": "Vue Boilerplate",
  "version": "1.2.0",
  "description": "This is a boilerplate for vue project. You could use it as a base to build your own web app.",
  "author": "William Chan <root@williamchan.me>, Emil Zhai <root@derzh.com>",
  "private": true,
  "scripts": {
    "setup": "sh ./cmd/setup.sh",
    "start": "npm run setup && npm run watch",
    "watch": "npm run watch:development --",
    "watch:development": "node webpack/runner.js --action=run --env=development --",
    "watch:production": "node webpack/runner.js --action=run --env=production --",
    "build": "npm run build:production --",
    "build:development": "node webpack/runner.js --action=build --env=development --public-path=/m/ --",
    "build:production": "node webpack/runner.js --action=build --env=production --public-path=/m/ --",
    "build:report": "node webpack/runner.js --action=build --env=production --public-path=/m/ --report --",
    "build:chrome-ext": "node webpack/runner.js --action=build --env=production --chrome-ext --",
    "lint": "npm run eslint && npm run stylelint",
    "lint:fix": "npm run eslint:fix && npm run stylelint:fix",
    "gitlint": "sh ./cmd/gitlint.sh || exit 1",
    "eslint": "eslint \"src/**/*.{js,jsx,ts,tsx,json}\" --format pretty",
    "eslint:fix": "eslint \"src/**/*.{js,jsx,ts,tsx,json}\" --format pretty --fix",
    "eslint:commit": "eslint --format pretty",
    "stylelint": "stylelint \"src/**/*.{less,sass,scss,css,vue}\" --custom-formatter=node_modules/stylelint-formatter-pretty",
    "stylelint:fix": "stylelint \"src/**/*.{less,sass,scss,css,vue}\" --custom-formatter=node_modules/stylelint-formatter-pretty --fix",
    "stylelint:commit": "stylelint --custom-formatter=node_modules/stylelint-formatter-pretty"
  },
  "engines": {
    "node": ">=12.0",
    "npm": ">=6.0"
  },
  "dependencies": {
    "axios": "^0.21.2",
    "cheerio": "^1.0.0-rc.10",
    "core-js": "^3.14.0",
    "eruda": "^2.4.1",
    "fastclick": "^1.0.6",
    "form-data": "^3.0.1",
    "fp-ts": "^2.10.5",
    "io-ts": "^2.2.16",
    "io-ts-reporters": "^2.0.1",
    "js-cookie": "^2.2.1",
    "lodash": "^4.17.21",
    "moment": "^2.29.4",
    "portal-vue": "^2.1.7",
    "querystringify": "^2.2.0",
    "regenerator-runtime": "^0.13.8",
    "safe-area-insets": "^1.4.1",
    "ua-parser-js": "^0.7.28",
    "uuid": "^8.3.2",
    "vue": "^2.6.14",
    "vue-class-component": "^7.2.6",
    "vue-lazyload": "^1.3.4",
    "vue-photoswipe.js": "^2.0.23",
    "vue-prevent-overscroll.js": "^2.0.5",
    "vue-property-decorator": "^9.1.2",
    "vue-router": "^3.5.1",
    "vue-wechat": "^1.6.1",
    "vuex": "^3.6.2",
    "vuex-class": "^0.3.2",
    "vuex-router-sync": "^5.0.0"
  },
  "devDependencies": {
    "@babel/core": "^7.14.5",
    "@babel/plugin-syntax-dynamic-import": "^7.8.3",
    "@babel/preset-env": "^7.14.5",
    "@babel/preset-typescript": "^7.14.5",
    "@babel/register": "^7.14.5",
    "@commitlint/cli": "^12.1.4",
    "@commitlint/config-conventional": "^12.1.4",
    "@types/cheerio": "^0.22.29",
    "@types/chrome": "0.0.145",
    "@types/fastclick": "^1.0.29",
    "@types/js-cookie": "^2.2.6",
    "@types/lodash": "^4.14.170",
    "@types/querystringify": "^2.0.0",
    "@types/ua-parser-js": "^0.7.36",
    "@types/uuid": "^8.3.0",
    "@typescript-eslint/eslint-plugin": "^4.26.1",
    "@typescript-eslint/parser": "^4.26.1",
    "@vue/babel-preset-jsx": "^1.2.4",
    "autoprefixer": "^10.2.6",
    "babel-eslint": "^10.0.3",
    "babel-loader": "^8.2.2",
    "cache-loader": "^4.1.0",
    "chalk": "^3.0.0",
    "compression-webpack-plugin": "^3.1.0",
    "connect-history-api-fallback": "^1.6.0",
    "copy-webpack-plugin": "^5.1.2",
    "cross-env": "^7.0.3",
